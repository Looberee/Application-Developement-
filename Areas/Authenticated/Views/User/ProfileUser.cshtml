@model List<WebApplication123.Models.ApplicationUser>;
@{
}

<h1>Customers List</h1>
<table class="table">
	<thead>
		<tr>
			<th>Email</th>
			<th>Name</th>
			<th>Address</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var customer in Model)
		{
			<tr>
				<td>@customer.Email</td>
				<td>@customer.FullName</td>
				<td>@customer.Address</td>
				<td>@customer.Role</td>
				<td>
				</td>
			</tr>
		}
	</tbody>
</table>